{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "/movies",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/register",
        "destination": "/",
        "type": 301
      },
      {
        "source": "/movies/*",
        "destination": "/",
        "type": 301
      }
    ]
  }
}